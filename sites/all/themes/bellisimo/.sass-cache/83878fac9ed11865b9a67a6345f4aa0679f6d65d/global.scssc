}

/* DRUPAL OVERIDES */
html body.admin-menu { margin-top: 0 !important; }

body.admin-menu { padding-top: 30px !important; }

ul.nice-menu-down li { border: none; }

nav#site-nav ul li { background: transparent; }

/* DRUPAL EXTRAS */
.field-name-field-blog-post-tags a { background: none repeat scroll 0 0 #80bb00; -moz-border-radius: 3px 3px 3px 3px; -webkit-border-radius: 3px 3px 3px 3px; -o-border-radius: 3px 3px 3px 3px; border-radius: 3px 3px 3px 3px; color: white; display: block; float: left; font-size: 11px; margin-right: 7px; padding: 0 7px 1px; }

/* DRUPAL MESSAGES */
div.messages { display: block; font-weight: bold; font-size: 13px; font-size: 1.3rem; border: 1px solid rgba(0, 0, 0, 0.1); margin-bottom: 12px; -moz-border-radius: 3px 3px 3px 3px; -webkit-border-radius: 3px 3px 3px 3px; -o-border-radius: 3px 3px 3px 3px; border-radius: 3px 3px 3px 3px; position: relative; background-color: #7fae00; color: white; text-shadow: rgba(0, 0, 0, 0.3) 0 -1px; }
div.messages a { color: white; text-decoration: underline; }
div.messages a:hover { text-decoration: none; }

div.warning { background-color: #c08c00; color: white; text-shadow: rgba(0, 0, 0, 0.5) 0 -1px; }

div.error { background-color: #c00000; color: white; text-shadow: rgba(0, 0, 0, 0.5) 0 -1px; }


/* OVERRIDES FOR DRUPAL ADMIN TABS - GOOGLE STYLE */
ul.tabs.primary {
  border: none;
  margin: 10px 0 20px 0;
}
ul.tabs.primary li.active a {
  border:none;
}
ul.tabs.primary li a {
   border: none;
   border: 1px solid #DDD;
   border-radius: 3px;
   text-shadow: 0 1px 1px white;
   -webkit-box-shadow: 0 1px 1px #fff;
   -moz-box-shadow:    0 1px 1px #fff;
   box-shadow:         0 1px 1px #fff;
   font: bold 11px Sans-Serif;
   padding: 6px 10px;
   white-space: nowrap;
   vertical-align: middle;
   color: #666;
   background: transparent;
   cursor: pointer;
}
ul.tabs.primary li a:hover, ul.tabs.primary li a:focus {
   border-color: #999;
   background: -webkit-linear-gradient(top, white, #E0E0E0);
   background:    -moz-linear-gradient(top, white, #E0E0E0);
   background:     -ms-linear-gradient(top, white, #E0E0E0);
   background:      -o-linear-gradient(top, white, #E0E0E0);
   -webkit-box-shadow: 0 1px 2px rgba(0,0,0,0.25), inset 0 0 3px #fff;
   -moz-box-shadow:    0 1px 2px rgba(0,0,0,0.25), inset 0 0 3px #fff;
   box-shadow:         0 1px 2px rgba(0,0,0,0.25), inset 0 0 3px #fff;
}
ul.tabs.primary li.active a, ul.tabs.primary li a:active{
   border: 1px solid #AAA;
   border-bottom-color: #CCC;
   border-top-color: #999;
   -webkit-box-shadow: inset 0 1px 2px #aaa;
   -moz-box-shadow:    inset 0 1px 2px #aaa;
   box-shadow:         inset 0 1px 2px #aaa;
   background: -webkit-linear-gradient(top, #E6E6E6, #DCDCDC);
   background:    -moz-linear-gradient(top, #E6E6E6, #DCDCDC);
   background:     -ms-linear-gradient(top, #E6E6E6, #DCDCDC);
   background:      -o-linear-gradient(top, #E6E6E6, #DCDCDC);
}
.node-links ul{
	padding: 0;
	margin: 0;
}
.node-links ul li {
	list-style-type: none;
	
}

body.not-front .camera_commands, body.not-front .camera_next, body.not-front .camera_prev {
	display: none !important;
}
.node-type-webform{
  section#section-content{
    #zone-content{
	    .ds-left{
		    margin: 0;
		    padding: 0;
	    }
	  }
	}
}
form{
	label{
		float: none;
		display: block;
		width: 110px;
		clear: both;
		padding: 0 10px 0 0 ;
		color: $redbar;
		font-size: 12px;
	}
	input, textarea{
		width: 100%;
	}
}
.form-textarea-wrapper textarea{
	width: 100%;
}
#webform-client-form-18{
	margin: 0;
	input[type="submit"]{
		margin: 0;
	}
}


