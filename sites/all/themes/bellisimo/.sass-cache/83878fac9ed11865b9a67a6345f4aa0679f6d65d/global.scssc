/* HOME RECENT PROJECTS*/
.ds-right{
	clear: both;
	.view-recent-projects-slider{
		.view-header{
		  background: none;
			h3{
				background: none;
				text-align: center;
				font-size: 24px;
				letter-spacing: normal;
				font-weight: normal;
				margin: 0;
			}
		}
		.view-content{
			position: relative;
			.project-link{
				position: absolute;
				a{
					display: block;
					height: 100%;
					width: 100%;
				}
			}
		}
  }
}
/* ----- TEAM ----- */
.view-team{
	.views-row{
		width: 100%;
		margin: 20px 0;
		height: 315px;
		h3{
		  padding: 0;
		  margin: 0;
		  border-bottom: solid 1px #ccc;
		  display: block;
		  a{
			  color: $redbar;
			  text-decoration: none;
			}
		}
		
	}
}
.field-name-field-team-job-title {
	font-weight: bold;
	font-size: 14px;
	line-height: 1.8em;
}


/* ------------ FOOTER SECTION ----------- */

#section-footer{
	background: url(../images/footer-ribbon.png) repeat-x;
	padding-top: 44px;
}


#zone-postscript-wrapper{
	background: #42000f;
	color: #990123
}
#zone-footer-wrapper{
	background:#210007;
	color:#990123;
	.site-by{
		text-align:right;
	}

}

/* FOOTER STYLES */
#section-footer{
  .block{
	  padding: 20px 0 0 0;
	  font-family: "PT Sans", Arial, sans-serif;
    h2.block-title{
	    color: #76031f;  
	    font-size: 27px;
	    font-weight: normal;
	    letter-spacing: normal;
	  }
	  p{
	    margin: 0;
		  a{
			  color: #fff;
			  text-decoration: none;
			  font-size: 16px;
			  font-family: Times, Georgia, serif;  
			  &:hover{
			    color: $redbar;
			  }
			}
		}
		.more-link{
			a{
				color: $redbar;
				text-decoration: none;
				padding: 10px 0;
        display: block;
        text-transform: uppercase;
        &:hover{
          color: #d11445;
        }
			}
		}
  }
  /*ADDRESS BLOCK*/
	#block-block-4{
		h2.block-title{
			background: url(../images/logo-footer.jpg) no-repeat;
			height: 49px;
			width: 139px;
			text-indent: -9999px;
		}
		.vcard{
		  color: #fff;
		  font-family: "PT Sans", Arial, sans-serif;
		  p{
