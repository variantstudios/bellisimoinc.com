:@template"D:/* This file will hold styles for the mobile version of your website (mobile first). */
/* This also can include ANY global CSS that applies site-wide. Unless overwritten by a more specific style rule, CSS declarations in global.css will apply site-wide. */

@import "camera.css";
@import "drupal-reset.css";
//@import "sass-inc";


/*----- MIXINS -----*/
@mixin hide-text {
	text-indent: 100%;
	white-space: nowrap;
	overflow: hidden;
}
.hide-text {
	text-indent: 100%;
	white-space: nowrap;
	overflow: hidden;
}
@mixin box-shadow-inset($properties, $properties2) {
  -webkit-box-shadow: $properties, $properties2;
  -moz-box-shadow: $properties, $properties2;
  -o-box-shadow: $properties, $properties2;
	box-shadow: $properties, $properties2;
}
@mixin box-shadow($properties) {
  -webkit-box-shadow: $properties;
  -moz-box-shadow: $properties;
  -o-box-shadow: $properties;
	box-shadow: $properties;
}
@mixin border-radius($properties) {
  -webkit-border-radius: $properties;
  -moz-border-radius: $properties;
  -o-border-radius: $properties;
  border-radius: $properties;
}
@mixin box-sizing{
  -webkit-box-sizing: border-box; /* Safari/Chrome, other WebKit */
	-moz-box-sizing: border-box;    /* Firefox, other Gecko */
	box-sizing: border-box;         /* Opera/IE 8+ */
}
@mixin transform-rotate($properties){
  transform:rotate($properties);
  -ms-transform:rotate($properties);
  -moz-transform:rotate($properties);
  -webkit-transform:rotate($properties);
  -o-transform:rotate($properties);
}
@mixin transition-duration($properties){
   transition-duration: ($properties);
	 -webkit-transition-duration: ($properties);
	 -moz-transition-duration: ($properties);
	 -o-transition-duration: ($properties);
	 -ms-transition-duration: ($properties);
	
}

/* ------------ VARIABLES ----------- */
$darkerred: #210007; //bottom footer bg
$darkred: #42000f; //top footer bg
$red: #7e0825; //page title bg?
$brightred: #990123; // top footer titles, bottom footer text
$gray: #3f3f3f; // text paragraphs

$redbar: #9e032b; // red for the bars

body,html{
	/*background:#f8f7f1 url('../images/header-images/wahoo.jpg') no-repeat center top;*/
	background:#f8f7f1;
}
.label-above{
	font-weight: bold;
}
span.file{
	display: block;
	clear:both;
}

/* ------------ HOME SLIDESHOW ----------- */

.camera_wrap {
    position: absolute;
    z-index: -10px;
}

.cameraContents{
	background: url(../images/bg-slider-fade.png) top repeat-x;
}

/* ------------ HEADER SECTION ----------- */


#zone-header-wrapper {
	background:rgba(255,255,255,0.9);
	/*height: 120px;*/
	border-bottom: $redbar solid 5px;
	position: relative;
	
	h1.site-name, h2.site-name {
    background: url(../images/logo.png) no-repeat 0 0;
		display: block;
		height: 80px;
		width: 226px;
	  @include hide-text;
		a {
			display: block;
			height: 80px;
			width: 226px;
		}
	}
}

/* ------------ MAIN MENU ----------- */
ul.nice-menu{
  padding: 60px 0 0 0 !important;
	a{
		padding: 5px 10px;
		background: none;
	}
	li{
		border: none;
		background: none;
		float: none;
		a{	
			font-family: "PT Sans";
			background: none;
			color: #000;
			text-decoration: none;
			text-transform: uppercase;
			display: block;
			&:hover{
				background: none;
				color: $redbar;
			}
		}
		/*DROP DOWNS*/
		ul{
			background: #fff;
			background: rgba(255,255,255,0.9);	
			padding: 10px;
			@include box-shadow(0 1px 2px #999);
			margin: 42px 0 0 100px !important;
			width: 250px;
			right: 0 !important;
			left: auto !important;
	    li{
	    	width: 250px;
		    a{
			    text-align: right;
			    text-transform: none;
			    line-height: 1.2em;
		    }  
		  }
	  }
	}
}


/* ------------ CONTENT SECTION ----------- */
section#section-content{
	margin: 420px 0 20px;
	#zone-content{
		padding: 0 0 10px 0;
		background: #fff;
		color: $gray;
		position: relative;
		-webkit-box-shadow: 1px 0px 5px rgba(50,50,50,0.5);
		-moz-box-shadow: 1px 0px 5px rgba(50,50,50,0.5);
		box-shadow: 1px 0px 5px rgba(50,50,50,0.5);
		.ds-left{
		  margin: 40px 0 0 28px;
		 	@include box-sizing;
		 	padding: 0 40px 0 0;
		 	/*img.logo{
			 	width: 30% !important;
			 	height: auto !important;
			 	float: left;
			 	margin: 0 2% 0 0;
		 	}*/
		}
	}
}

.logo{
	line-height: 6em;
	float: left;
	margin: 10px;
	img{
		vertical-align: middle;
	}
}


/* ----- MAIN CONTENT ----- */

.breadcrumb{
	display: none;
}

.ds-header {
   background: none repeat scroll 0 0 $redbar;
    /*margin: 0 -10px !important;*//*use after mobile*/
    margin: 0 0 0 -10px;
    /*padding: 0 !important; use after mobile */
    padding: 0 20px 0 0;
    width: 100% !important;
    h2{
    	padding: 10px 0px 0px 20px;
    	font-size: 26px;
    	font-weight: normal;
    	color: #fff;
    	margin-bottom: 12px;
    	letter-spacing: normal;
    }
}
.ds-left{
  .field{
	  clear: both;
  }
  .label-inline,
	.label-above{
		display: block;
		font-weight: 700;
		padding: 0 5px 0 0;
		float: left;
	}
	.field-name-field-project-location{
		overflow: hidden;
	}
	
	
}

.ds-right{
  @include box-sizing;
	padding: 40px 0px 0px 0;
	.flex-nav-container{
    .slides{
		  li{
		  	height: 100% !important;
	  	}
  	}
  	
 }
	.field-name-field-project-logo{
		padding: 0 0 20px 0;
		text-align: center;
	}
	
	.field-slideshow-wrapper{
	  .field-slideshow{
	    background: url(../images/project-slider-shadow.jpg) bottom no-repeat;	
	    height: 240px;
	    padding: 0 0 8px 0 !important;
	    position: relative !important;
	    .field-slideshow-caption{
		    position: absolute;
		    bottom: 0;
		    background: #000;
		    background: rgba(0,0,0,0.5);
		    color: #fff;
		    text-align: center;
		    display: block;
		    width: 230px;
		    padding: 10px 0;  
		  }
	  }
	}
}

/*----- FLEXSLIDER -----*/
.flex-direction-nav{
	li{
	  .prev{
		  left: -12px;
	  }
	  .next{
		  right: -12px;
	  }
	}
}

/* ----- FEATURED PROJECT OVERRIDES ----- */
.view-mode-featured_project{
	.ds-header:after{
		background: url(../images/featured-tag.png) no-repeat;
		display: block;
		content: "";
		height: 141px;
		width: 69px;
		position: absolute;
		right: 10px;
		top: -10px;
		z-index: 1;
	}
	.ds-left{
		border-right: none !important;
		
	}
	.ds-right{
	  @include box-sizing;
	  clear:both;
	  .field-name-field-project-details{
		  width: 90%;
	  } 
	}
}

.term-listing-heading,
.view-header{
  background: none repeat scroll 0 0 $redbar;
  margin: 0 -10px !important;
  padding: 0 10px !important;
  width: 100% !important;
  h2{
		padding: 10px 0px 12px 10px;
  	font-size: 24px;
  	font-weight: normal;
  	color: #fff;
  	margin-bottom: 12px;
  	letter-spacing: normal;
	}
}
.term-listing-heading{
  margin-bottom: 40px !important;
 	h2{
		padding: 10px 0 0 10px;
	}
}


/* ------ PROJECT VIEW TAXONOMY ------ */

#region-content{
  
  .view-bellisimo-projects{
	  .view-content{
	    margin: 0 0 0 0;
	    padding: 40px 0 0 0;
	  }
		.views-row{
		  background: #eee;
			float: left;
			width: 85%;
			height: 100%;
			margin: 0 0 40px 5%;
			padding: 0 10px 10px;
			@include border-radius(6px);
			@include transition-duration(0.5s);
			&:hover{
			  background: #ccc;
			  @include box-shadow(0 1px 0 #999);
			  @include transition-duration(0.5s);
			}
			h3{
			  height: 21px;
			  line-height: 1em;
			  padding: 10px 0 0 0;
				a{
				  color: $redbar;
			    text-decoration: none;
			    font-family: "PT Sans", Arial, sans-serif;
			  	font-size: 14px;
				  font-weight: normal;
				  text-transform: uppercase;
				  line-height: 1em;
				  letter-spacing: normal;
				  height: 209px;
				  display: block;
				  text-align: center;
				  @include transition-duration(0.5s);
				  position: relative;
				  z-index: 1;
				  &:hover{
				    color: #333;
				    @include transition-duration(0.5s);
				  }
			  }
			}
		}
		.views-row:nth-child(3n){
		  margin: 0 0 40px 5%;
		}
	}
}
