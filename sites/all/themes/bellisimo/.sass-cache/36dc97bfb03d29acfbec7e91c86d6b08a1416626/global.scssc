@import "camera.css";
@import "drupal-reset.css";
//@import "sass-inc";

p  {
	margin-bottom: 10px;
}

/*----- MIXINS -----*/
@mixin hide-text {
	text-indent: 100%;
	white-space: nowrap;
	overflow: hidden;
}
.hide-text {
	text-indent: 100%;
	white-space: nowrap;
	overflow: hidden;
}
@mixin box-shadow-inset($properties, $properties2) {
  -webkit-box-shadow: $properties, $properties2;
  -moz-box-shadow: $properties, $properties2;
  -o-box-shadow: $properties, $properties2;
	box-shadow: $properties, $properties2;
}
@mixin box-shadow($properties) {
  -webkit-box-shadow: $properties;
  -moz-box-shadow: $properties;
  -o-box-shadow: $properties;
	box-shadow: $properties;
}
@mixin border-radius($properties) {
  -webkit-border-radius: $properties;
  -moz-border-radius: $properties;
  -o-border-radius: $properties;
  border-radius: $properties;
}
@mixin box-sizing{
  -webkit-box-sizing: border-box; /* Safari/Chrome, other WebKit */
	-moz-box-sizing: border-box;    /* Firefox, other Gecko */
	box-sizing: border-box;         /* Opera/IE 8+ */
}
@mixin transform-rotate($properties){
  transform:rotate($properties);
  -ms-transform:rotate($properties);
  -moz-transform:rotate($properties);
  -webkit-transform:rotate($properties);
  -o-transform:rotate($properties);
}
@mixin transition-duration($properties){
   transition-duration: ($properties);
	 -webkit-transition-duration: ($properties);
	 -moz-transition-duration: ($properties);
	 -o-transition-duration: ($properties);
	 -ms-transition-duration: ($properties);
	
}

/* ------------ VARIABLES ----------- */
$darkerred: #210007; //bottom footer bg
$darkred: #42000f; //top footer bg
$red: #7e0825; //page title bg?
$brightred: #990123; // top footer titles, bottom footer text
$gray: #3f3f3f; // text paragraphs

$redbar: #9e032b; // red for the bars

body,html{
