/* ------------ HEADER SECTION ----------- */


#zone-header-wrapper {
	background:rgba(255,255,255,0.9);
	/*height: 120px;*/
	border-bottom: $redbar solid 5px;
	position: relative;
	
	h1.site-name, h2.site-name {
    background: url(../images/logo.png) no-repeat 0 0;
		display: block;
		height: 80px;
		width: 226px;
